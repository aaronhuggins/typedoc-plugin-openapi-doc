  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/js-yaml": "^4.0.0",
    "@types/jsdom": "^16.2.6",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.25",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
